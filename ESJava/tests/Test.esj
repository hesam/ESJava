import java.util.ArrayList;
import polyglot.ext.esj.primitives.*;
import polyglot.ext.esj.tologic.*;

class Test {
    
    protected ESJList nums;

    public Test(){
	super();
	this.nums = new ESJList();
    }
    
    public ESJList nums() {
	return nums;
    }

    public LogSet nums_log() {
	return new LogSet("A?.r?");
    }

    predicate public boolean isFoo(int j) {
	//nums.get(0) > j
	    all w : nums() | w > j 
	    //&& nums.indices().allButLast().get(0) < j
    }

    public void fallback() {
	System.out.println("--> fallback initiated...");
    }

    ensured public void m1() 
	ensures isFoo(3) {
	nums.add(0);
    }

    public static void main(String[] args) {
	Test t1 = new Test();
	t1.nums.add(0);
	t1.nums.add(3);
	t1.nums.add(5);
	t1.nums.add(1);
	//System.out.println(t1.nums.indices_log().allButLast_log());
	//System.out.println(t1.isFoo_log(new LogInt("2")));
    }

}
