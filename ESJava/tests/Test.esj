class Test {
    
    protected ESJList nums;

    public ESJList nums() {
	return nums;
    }

    predicate public boolean isFoo(int j) {
	//all w : nums() | w > j &&
	nums.get(0) < j
    }

    public void fallback() {
	System.out.println("--> fallback initiated...");
    }

    ensured public void m1() 
	ensures isFoo(3) {
	nums.add(0);
    }

    public static void main(String[] args) {
	Test t1 = new Test();
	//t1.m1();
