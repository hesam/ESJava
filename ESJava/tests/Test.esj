import java.util.ArrayList;
import polyglot.ext.esj.primitives.*;

class Test {
    
    protected ESJList nums;

    public ESJList nums() {
	return nums;
    }

    predicate public boolean isFoo(int j) {
	all i : nums() | this.nums().get(i) < j
        //this.nums().get(0) < 0
    }

    public void fallback() {
	System.out.println("--> fallback initiated...");
    }

    ensured public void m1() 
	ensures isFoo(3) {
	nums.add(0);
    }

    public static void main(String[] args) {
	Test t1 = new Test();
	//t1.m1();
    }

}
